<?xml version="1.0" encoding="ISO-8859-1"?>

<plugin xmlns="http://www.willuhn.de/schema/jameica-plugin"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.willuhn.de/schema/jameica-plugin http://www.willuhn.de/schema/jameica-plugin-1.2.xsd"
        name="hibiscus.sync.fidorcsv" version="0.3.0" class="org.jameica.hibiscus.sync.fidorcsv.Plugin">

  <description>Hibiscus-Support für Fidor CSV-Import</description>
  <url>http://hibiscus.tvbrowser.org/download/hibiscus.sync.fidorcsv.zip</url>
  <homepage>https://github.com/ds10git/fidorcsv</homepage>
  <license>GPL v2</license>

  <requires jameica="2.6.0+">
    <import plugin="hibiscus" version="2.6.0+" />
  </requires>

  <classfinder>
    <include>hibiscus\.sync\.fidorcsv\.jar</include>
    <include>.*\.class</include>
  </classfinder>

</plugin>
